{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["-f", "short1-bal.rep"],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/ubuntu/week06-malloclab",
      "preLaunchTask": "build",
      "program": "/home/ubuntu/week06-malloclab/mdriver",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
